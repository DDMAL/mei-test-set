<?xml-model href="http://music-encoding.googlecode.com/svn/tags/MEI2013_v2.1.1/schemata/mei-all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="2013">
    <meiHead>
        <fileDesc>
            <titleStmt>
                <title>Example of an encoded slur</title>
                <respStmt>
                    <persName role="composer"> Claude Debussy </persName>
                    <persName role="encoder">Maja Hartwig</persName>
                </respStmt>
            </titleStmt>
            <pubStmt/>
            <seriesStmt>
                <title>MEI Sample Collection</title>
                <respStmt>
                    <corpName role="publisher">MEI Project</corpName>
                    <corpName role="funder">German Research
                        Foundation<address>
                     <addrLine>Kennedyallee 40</addrLine>
                     <addrLine>
                        <geogName>Bonn</geogName>
                     </addrLine>
                     <addrLine>
                        <geogName>Germany</geogName>
                     </addrLine>
                  </address>
                    </corpName>
                    <corpName role="funder">National Endowment for the
                        Humanities<address>
                     <addrLine>1100 Pennsylvania Avenue N.W.</addrLine>
                     <addrLine>
                        <geogName>Washington, DC</geogName> 20004</addrLine>
                     <addrLine>
                        <geogName>United States</geogName>
                     </addrLine>
                  </address>
                    </corpName>
                </respStmt>
            </seriesStmt>
            <sourceDesc>
                <source n="1">
                    <identifier type="URI" label="print"
                        >http://conquest.imslp.info/files/imglnks/usimg/a/a0/IMSLP02907-Debussy-L075.pdf</identifier>
                    <titleStmt>
                        <title>Suite Bergamesque</title>
                        <respStmt>
                            <persName>Claude Debussy</persName>
                        </respStmt>
                    </titleStmt>
                </source>
            </sourceDesc>
        </fileDesc>
        <encodingDesc>
            <editorialDecl>
                <p>The example represents the encoding of a special slur in measure 2 (measure 20 in
                    the image) in the sample as well as clef changes.</p>
            </editorialDecl>
        </encodingDesc>
        <revisionDesc>
            <change n="1">
                <respStmt>
                    <persName>Maja Hartwig</persName>
                </respStmt>
                <changeDesc>
                    <p>Encoding of the example.</p>
                </changeDesc>
                <date isodate="2013-26-04"/>
            </change>
        </revisionDesc>
    </meiHead>
    <music>
        <body>
            <mdiv>
                <score>
                    <section>
                        <scoreDef key.sig="1f" meter.count="4" meter.unit="4">
                            <staffGrp barthru="true" symbol="brace">
                                <staffDef n="1" lines="5" clef.shape="G" clef.line="2"/>
                                <staffDef n="2" clef.shape="F" clef.line="4" lines="5"/>
                            </staffGrp>
                        </scoreDef>
                        <measure n="1">
                            <staff n="1">
                                <layer n="1">
                                    <note pname="f" oct="4" dur="2" stem.dir="up" xml:id="m1s1l1e1"
                                        tie="i"/>
                                    <note pname="f" oct="4" dur="4" stem.dir="up" xml:id="m1s1l1e2"
                                        tie="t"/>
                                    <rest dur="4" xml:id="m1s1l1e3" loc="6"/>
                                </layer>
                                <layer n="2">
                                    <chord dur="4" stem.dir="down" xml:id="m1s1l2e1">
                                        <note pname="b" oct="3"/>
                                        <note pname="d" oct="4"/>
                                    </chord>
                                    <beam>
                                        <chord dur="8" stem.dir="down" xml:id="m1s1l2e2">
                                            <note pname="a" oct="3"/>
                                            <note pname="c" oct="4"/>
                                        </chord>
                                        <chord dur="8" stem.dir="down" xml:id="m1s1l2e3">
                                            <note pname="g" oct="3"/>
                                            <note pname="b" oct="3"/>
                                        </chord>
                                    </beam>
                                    <chord dur="2" stem.dir="down" xml:id="m1s1l2e4">
                                        <note pname="a" oct="3"/>
                                        <note pname="c" oct="4"/>
                                    </chord>
                                </layer>
                            </staff>
                            <staff n="2">
                                <layer n="1">
                                    <note pname="f" oct="2" dur="8" stem.dir="up" xml:id="m1s2l1e1"/>
                                    <note pname="c" oct="3" dur="4" stem.dir="up" xml:id="m1s2l1e2"/>
                                    <note pname="f" oct="3" dur="8" stem.dir="up" tie="i"
                                        xml:id="m1s2l1e3"/>
                                    <note pname="f" oct="3" dur="8" stem.dir="up" tie="t"
                                        xml:id="m1s2l1e4"/>
                                    <note pname="c" oct="4" dur="4" stem.dir="up" xml:id="m1s2l2e5"/>
                                    <note pname="f" oct="4" dur="4" stem.dir="up" xml:id="m1s2l2e6"
                                    />
                                </layer>
                                <layer n="2">
                                    <note pname="f" oct="1" dur="1" xml:id="m1s2l2e1"/>
                                    <clef shape="G" line="2"/>
                                </layer>
                            </staff>
                            <slur staff="2" curvedir="below" layer="1" tstamp="0.5" tstamp2="0m+1.5"/>
                            <slur staff="2" curvedir="below" layer="1" tstamp="2" tstamp2="0m+4.5"/>
                            <hairpin form="dim" staff="2" place="above" tstamp="3" tstamp2="4.5"/>
                        </measure>
                        <measure n="2">
                            <staff n="1">
                                <layer n="1">
                                    <note pname="e" oct="6" stem.dir="up" dur="8" xml:id="m2s1l1e1"/>
                                    <note pname="b" accid="n" oct="5" stem.dir="up" dur="4"
                                        xml:id="m2s1l1e2"/>
                                    <note pname="a" oct="5" dur="8" tie="i" stem.dir="up"
                                        xml:id="m2s1l1e3"/>
                                    <beam>
                                        <note pname="a" oct="5" dur="8" stem.dir="up" tie="t"
                                            xml:id="m2s1l1e4"/>
                                        <note pname="e" oct="5" dur="8" stem.dir="up"
                                            xml:id="m2s1l1e5"/>
                                    </beam>
                                    <beam>
                                        <note pname="b" accid="n" oct="4" dur="32" stem.dir="up"
                                            xml:id="m2s1l1e6"/>
                                        <note pname="a" oct="4" dur="32" stem.dir="up"
                                            xml:id="m2s1l1e7"/>
                                        <note pname="b" oct="4" dur="32" stem.dir="up"
                                            xml:id="m2s1l1e8"/>
                                        <note pname="d" oct="5" dur="32" stem.dir="up"
                                            xml:id="m2s1l1e9"/>
                                    </beam>
                                    <rest dur="8" xml:id="m2s1l1e10" loc="6"/>
                                </layer>
                                <layer n="2">
                                    <note pname="e" oct="5" dur="2" dots="1" stem.dir="down"
                                        xml:id="m2s1l2e1"/>
                                    <note pname="f" oct="4" dur="8" stem.dir="down"
                                        xml:id="m2s1l2e2"/>
                                </layer>
                            </staff>
                            <staff n="2">
                                <layer n="1">
                                    <mSpace/>
                                </layer>
                                <layer n="2">
                                    <chord dur="2" stem.dir="up" dots="1" xml:id="m2s2l2e1">
                                        <note pname="a" oct="3" xml:id="m2s2l2c1e1"/>
                                        <note pname="e" oct="4" xml:id="m2s2l2c1e2"/>
                                        <note pname="c" oct="5" xml:id="m2s2l2c1e3"/>
                                    </chord>
                                    <clef shape="F" line="4"/>
                                    <beam>
                                        <chord dur="8" stem.dir="down" xml:id="m2s2l2e2">
                                            <note pname="d" oct="3"/>
                                            <note pname="a" oct="3"/>
                                        </chord>
                                        <clef shape="G" line="2"/>
                                        <chord dur="8" stem.dir="down" xml:id="m2s2l2c2e3">
                                            <note pname="a" oct="3"/>
                                            <note pname="c" oct="4"/>
                                            <note pname="d" oct="4" xml:id="m2s2l2c2e1"/>
                                        </chord>
                                    </beam>
                                </layer>
                            </staff>
                            <slur staff="1" curvedir="above" tstamp="0.5" tstamp2="0m+3"/>
                            <slur staff="1 2" startid="#m2s1l2e2" endid="#m2s2l2c2e1"/>
                            <!-- the slur could be marked up additionally with @bulge and @bezier -->
                            <dynam staff="1" place="below" tstamp="1">p</dynam>
                            <dir staff="1" place="above" tstamp="1">a tempo</dir>
                            <gliss staff="2" layer="2" startid="#m2s2l2c2e1" endid="#m2s2l2c1e3"/>
                        </measure>
                        <measure n="3">
                            <staff n="1">
                                <layer n="1">
                                    <note pname="e" oct="6" dur="8" stem.dir="up" xml:id="m3s1l1e1"/>
                                    <note pname="b" oct="5" dur="4" accid="n" stem.dir="up"
                                        xml:id="m3s1l1e2"/>
                                    <note pname="a" oct="5" dur="8" stem.dir="up" tie="i"
                                        xml:id="m3s1l1e3"/>
                                    <beam>
                                        <note pname="a" oct="5" dur="8" stem.dir="up" tie="t"
                                            xml:id="m3s1l1e4"/>
                                        <note pname="e" oct="5" dur="8" stem.dir="up"
                                            xml:id="m3s1l1e5"/>
                                        <note pname="b" oct="4" accid="n" dur="8" stem.dir="up"
                                            xml:id="m3s1l1e6"/>
                                        <note pname="a" oct="4" tie="i" stem.dir="up" dur="8"
                                            xml:id="m3s1l1e7"/>
                                    </beam>
                                </layer>
                                <layer n="2">
                                    <note pname="e" oct="5" dur="2" stem.dir="down" dots="1"
                                        xml:id="m3s1l2e1"/>
                                    <note pname="f" oct="4" dur="4" stem.dir="down" tie="i"
                                        xml:id="m3s1l2e2"/>
                                </layer>
                            </staff>
                            <staff n="2">
                                <layer n="1">
                                    <mSpace/>
                                    <chord dur="2" dots="1" xml:id="m3s2l2e1">
                                        <note pname="a" oct="3" xml:id="m3s2l2c1e1"/>
                                        <note pname="e" oct="3" xml:id="m3s2l2c1e2"/>
                                        <note pname="c" oct="5" xml:id="m3s2l2c1e3"/>
                                    </chord>
                                    <clef shape="F" line="4"/>
                                    <chord dur="4" stem.dir="down">
                                        <note pname="d" oct="3" xml:id="m3s2l2c2e1"/>
                                        <note pname="c" oct="4" xml:id="m3s2l2c2e2"/>
                                    </chord>
                                </layer>
                            </staff>
                            <slur staff="1" curvedir="above" tstamp="0.5" tstamp2="0m+4.5"/>
                            <slur staff="2" curvedir="below" tstamp="0.5" tstamp2="0m+4"/>
                        </measure>
                    </section>
                </score>
            </mdiv>
        </body>
    </music>
</mei>
