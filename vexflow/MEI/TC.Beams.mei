<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="../odd/mei2vexflow.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<?xml-model href="../odd/mei2vexflow.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<mei meiversion="2013" xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns="http://www.music-encoding.org/ns/mei" xmlns:mei="http://www.music-encoding.org/ns/mei">
	<meiHead>
		<fileDesc>
			<titleStmt>
				<title>MeiToVexFlow Beams Test Case</title>
			</titleStmt>
			<pubStmt>
				<respStmt>
					<resp>Prepared by</resp>
					<persName>Alexander Erhard</persName>
				</respStmt>
			</pubStmt>
		</fileDesc>
	</meiHead>
    <music>
        <body>
        	<mdiv>
        		<score>
        			<scoreDef page.width="150" page.height="70" page.leftmar="8" page.topmar="10" page.scale="50%">
        				<staffGrp symbol="none">
        					<staffDef n="1" lines="5" clef.line="2" clef.shape="G" meter.count="4" meter.unit="4" meter.rend="invis" clef.visible="false" key.pname="c" key.mode="major"/>
        				</staffGrp>
        			</scoreDef>
        			<section>
        				<measure n="1">
        					<staff n="1">
        						<layer>
        							<beam>
        								<note pname="c" oct="4" dur="32" slur="i1" stem.dir="down" ho="2"/>
        								<note pname="f" oct="4" dur="32" slur="t1" stem.dir="down"/>
        								<note pname="f" oct="4" dur="16" stem.dir="down"/>
        								<rest dur="16"/>
        								<note pname="g" oct="4" dur="32" stem.dir="down" slur="i1"/>
        								<note pname="c" oct="4" dur="32" stem.dir="down"/>
        							</beam>
        						</layer>
        					</staff>
        				</measure>
        				<measure n="2" right="invis" width="30">
        					<staff n="1">
        						<layer>
        							<note pname="a" oct="4" dur="4" slur="t1" stem.dir="down"/>
        						</layer>
        					</staff>
        				</measure>
        			</section>
        		</score>
        	</mdiv>
        </body>
    </music>
</mei>